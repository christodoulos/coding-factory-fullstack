{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rimraf dist",
    "watch-lib:-interfaces": "ng build interfaces --watch",
    "watch-lib:ui": "ng build ui --watch",
    "watch-lib:ui-forms": "wait-on dist/ui/public-api.d.ts && wait-on dist/interfaces/public-api.d.ts && ng build ui-forms --watch",
    "serve:app": "wait-on dist/interfaces/public-api.d.ts && wait-on dist/ui/public-api.d.ts && wait-on dist/ui-forms/public-api.d.ts && ng serve",
    "watch-all": "npm run clean && run-p watch-lib:* serve:app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.2.1",
    "@angular/compiler-cli": "^14.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/jasmine": "~4.0.0",
    "@types/lodash-es": "^4.17.6",
    "autoprefixer": "^10.4.9",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.2"
  }
}
